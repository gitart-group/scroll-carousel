{
  "name": "gitart-scroll-carousel",
  "version": "1.2.4",
  "scripts": {
    "dev": "vite --config dev/vite.config.ts",
    "dev:build": "vite build --config dev/vite.config.ts",
    "build": "node build/build.js",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --p ./build/tsconfig.json --outFile dist/index.d.ts",
    "lint": "yarn stylelint && yarn eslint",
    "stylelint": "stylelint src/**/*.vue",
    "eslint": "eslint . --ext .js,.ts,.vue",
    "test": "vitest"
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./bundled": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./dist": "./dist/*",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "bundled": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "gitart carousel",
    "carousel",
    "slider",
    "carousel component",
    "slider component",
    "vue slider",
    "vue3 slider",
    "vue carousel",
    "vue3 carousel"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gitart-group/scroll-carousel.git",
    "directory": "."
  },
  "author": {
    "name": "Michael Gitart",
    "email": "michaelgitart@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@gitart/eslint-config-vue": "0.1.1",
    "@types/node": "^17.0.5",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/runtime-core": "^3.2.26",
    "@vue/test-utils": "^2.0.0-rc.18",
    "eslint": "^8.16.0",
    "happy-dom": "^2.25.1",
    "sass": "^1.45.1",
    "shelljs": "^0.8.4",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.7.2",
    "unplugin-auto-import": "^0.5.5",
    "unplugin-vue-components": "^0.17.11",
    "vite": "2.9.9",
    "vite-plugin-checker": "^0.4.6",
    "vite-plugin-terminal": "^1.0.1",
    "vite-plugin-windicss": "^1.8.4",
    "vitest": "^0.13.0",
    "vue": "^3.2.36",
    "vue-tsc": "^0.35.2",
    "windicss": "^3.4.0"
  },
  "peerDependencies": {
    "vue": "^3.2.6"
  },
  "dependencies": {
    "@vueuse/core": "^8.2.0",
    "bezier-easing": "^2.1.0",
    "gitart-animate-number": "^0.1.0"
  }
}
